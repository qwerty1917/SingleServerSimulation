<div class="container">
    <div class="page-header">
        <h1>Results: <small>Simulation by Ruby on Rails</small></h1>
    </div>
    
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Total production</h3>
        </div>
        <div class="panel-body">
            <%= @state.get_completed%>
        </div>
    </div>
    
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Average waiting time</h3>
        </div>
        <div class="panel-body">
            <%= (@state.get_TofWTinQ/@state.get_NofCWTinQ).round(2) %>
        </div>
    </div>
    
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Maximum waiting time</h3>
        </div>
        <div class="panel-body">
            <%= @state.get_max_waiting_time%>
        </div>
    </div>
    
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Time-average number of parts in queue</h3>
        </div>
        <div class="panel-body">
            <%= (@state.get_Area_und_q_t/@state.get_clock).round(2) %>
        </div>
    </div>
    
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Maximum number of parts in queue</h3>
        </div>
        <div class="panel-body">
            <%= @state.get_max_num_in_queue%>
        </div>
    </div>
    
    <div class="panel panel-default">
        <div class="panel-heading">
            <h3 class="panel-title">Utilization of the machine</h3>
        </div>
        <div class="panel-body">
            <%= (@state.get_Area_und_b_t/@state.get_clock).round(2) %>
        </div>
    </div>
</div>